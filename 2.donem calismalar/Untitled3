#include <iostream>

float usalma(float sayi,float us)
{
	int i,sonuc=1;
	
	for(i=1;i<=us;i++)
	{
		sonuc*=sayi;
	}
	printf("Girilen sayi ile us nun sonucu=%d",sonuc);
}


int main()
{
	float sayi,n;
	
	printf("Bir sayi gir:");
	scanf("%f",&sayi);
	printf("Us gir:");
	scanf("%f",&n);
	
	usalma(sayi,n);
	
}
